{
  "name": "blog",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "postinstall": "npm link ./builders && npm run build:builders && npm link ./schematics && npm run build:schematics",
    "generate-post": "ng g @wellwind/blog-schematics:generate-post",
    "prerender": "ng run blog:prerender",
    "prerender:pages": "ng run blog:prerender --routes-file pages.txt --configuration static",
    "dev:ssr": "ng run blog:serve-ssr",
    "serve:ssr": "node dist/blog/server/main.js",
    "build:ssr": "ng build && ng run blog:server",
    "serve:assets": "ng run blog:static-server --path ./src --port 4201",
    "serve:blog": "ng run blog:static-server --path ./dist/blog/browser --port 4202",
    "build:builders": "cd builders && npm run build",
    "build:schematics": "cd schematics && npm run build",
    "build:blog-posts-json": "ng run blog:generate-blog-posts-json",
    "build:urls": "ng run blog:generate-prerender-urls",
    "build:rss": "ng run blog:generate-rss",
    "build:minify-html": "ng run blog:minify-rendered-files",
    "build:blog": "npm run build:blog-posts-json && npm run build:urls && npm run prerender:pages && npm run build:minify-html && npm run build:rss -- --rss-path ./dist/blog/browser/blog/atom.xml",
    "build:deploy": "npx concurrently \"npm run serve:assets\" \"npm run build:blog\" --kill-others"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.0.0",
    "@angular/cdk": "^13.0.0",
    "@angular/common": "~13.0.0",
    "@angular/compiler": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/material": "^13.0.0",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/platform-server": "~13.0.0",
    "@angular/router": "~13.0.0",
    "@nguniversal/express-engine": "^13.0.0",
    "feed": "^4.2.2",
    "html-minifier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "markdown-it": "^12.3.2",
    "ramda": "^0.27.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.0.1",
    "@angular/cli": "~13.0.1",
    "@angular/compiler-cli": "~13.0.0",
    "@nguniversal/builders": "^13.0.0",
    "@types/express": "^4.17.13",
    "@types/html-minifier": "^4.0.2",
    "@types/jasmine": "~3.8.0",
    "@types/js-yaml": "^4.0.3",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^12.11.1",
    "@types/puppeteer": "^5.4.4",
    "@types/ramda": "^0.27.45",
    "concurrently": "^6.3.0",
    "express": "^4.17.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "typescript": "~4.4.4"
  }
}
