{
  "name": "blog",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "postinstall": "npm run build:builders && npm run build:schematics && npm i ./builders && npm i ./schematics",
    "generate-post": "ng g @wellwind/blog-schematics:generate-post",
    "build:builders": "cd builders && npm run build",
    "build:schematics": "cd schematics && npm run build",
    "build:blog-posts-json": "ng run blog:generate-blog-posts-json",
    "build:urls": "ng run blog:generate-prerender-urls",
    "build:rss": "ng run blog:generate-rss",
    "build:minify-html": "ng run blog:minify-rendered-files",
    "build:deploy": "npm run build:blog-posts-json && npm run build:urls && npm run build && npm run build:rss -- --rss-path ./dist/blog/browser/browser/atom.xml && shx mv ./dist/blog/browser/browser/index.csr.html ./dist/blog/browser/browser/index.html",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.3",
    "@angular/cdk": "^18.2.3",
    "@angular/common": "^19.0.3",
    "@angular/compiler": "^19.0.3",
    "@angular/core": "^19.0.3",
    "@angular/forms": "^19.0.3",
    "@angular/material": "^18.2.3",
    "@angular/platform-browser": "^19.0.3",
    "@angular/platform-browser-dynamic": "^19.0.3",
    "@angular/platform-server": "^19.0.3",
    "@angular/router": "^19.0.3",
    "@angular/ssr": "^19.0.3",
    "@wellwind/blog-builder": "file:builders",
    "@wellwind/blog-schematics": "file:schematics",
    "feed": "^4.2.2",
    "highlight.js": "^11.9.0",
    "html-minifier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "marked": "^9.1.6",
    "marked-highlight": "^2.0.6",
    "ramda": "^0.30.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.3",
    "@angular-eslint/builder": "18.3.1",
    "@angular-eslint/eslint-plugin": "18.3.1",
    "@angular-eslint/eslint-plugin-template": "18.3.1",
    "@angular-eslint/schematics": "18.3.1",
    "@angular-eslint/template-parser": "18.3.1",
    "@angular/cli": "^19.0.3",
    "@angular/compiler-cli": "^19.0.3",
    "@types/express": "^4.17.13",
    "@types/html-minifier": "^4.0.2",
    "@types/jasmine": "~3.8.0",
    "@types/js-yaml": "^4.0.3",
    "@types/node": "^20.9.0",
    "@types/puppeteer": "^5.4.4",
    "@types/ramda": "^0.30.0",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "autoprefixer": "^10.4.12",
    "concurrently": "^6.3.0",
    "eslint": "^8.53.0",
    "express": "^4.17.1",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "postcss": "^8.4.17",
    "shx": "^0.3.3",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.2.1",
    "typescript": "~5.6.3"
  }
}
