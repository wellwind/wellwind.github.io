{
  "name": "blog",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prerender": "ng run blog:prerender",
    "prerender:urls": "ng run blog:prerender --routes-file urls.txt --configuration static",
    "dev:ssr": "ng run blog:serve-ssr",
    "serve:ssr": "node dist/blog/server/main.js",
    "build:ssr": "ng build && ng run blog:server",
    "build:blog-posts:json": "ts-node --project scripts/tsconfig.scripts.json scripts/generate-blog-posts-json.ts",
    "serve:assets": "ts-node --project scripts/tsconfig.scripts.json scripts/static-server.ts",
    "serve:blog": "ts-node --project scripts/tsconfig.scripts.json scripts/blog-server.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/platform-server": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@nguniversal/express-engine": "^12.1.1",
    "markdown-it": "^12.2.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.4",
    "@angular/cli": "~12.2.4",
    "@angular/compiler-cli": "~12.2.0",
    "@nguniversal/builders": "^12.1.1",
    "@types/connect": "^3.4.35",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jasmine": "~3.8.0",
    "@types/js-yaml": "^4.0.3",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^12.11.1",
    "@types/puppeteer": "^5.4.4",
    "@types/serve-static": "^1.13.10",
    "connect": "^3.7.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jasmine-core": "~3.8.0",
    "js-yaml": "^4.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "serve-static": "^1.14.1",
    "ts-node": "^10.2.1",
    "typescript": "~4.3.5"
  }
}
